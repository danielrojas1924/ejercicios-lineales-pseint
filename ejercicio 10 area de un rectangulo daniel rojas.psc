Algoritmo ejercicio_10_area_de_un_rectangulo
	
	// definir variables
	
	definir area,base,altura Como Real
	Escribir " base del rectangulo";
	leer base;
	escribir "altura del rectangulo"
	leer altura;
	area <- base * altura;
	Escribir " el area del rectangulo es : ", area;
	
FinAlgoritmo
