Algoritmo ejercicio_22_costo_de_impresion
	// definir variables
	
	definir cant_pag,precio_x_pag,costo Como Real;
	
	Escribir " ingrese la cantidad de paginas ";
	Leer cant_pag;
	Escribir " ingrese el valor de cada pagina"; 
	Leer precio_x_pag;
	costo <- cant_pag * precio_x_pag
	Escribir " el costo de impresion es $ :" , costo  " pesos "
	
FinAlgoritmo
