Algoritmo erecicio_11_temperatura
	
	// definir variables
	definir x,z,celsius,fahrenheit Como Real
	escribir "ingrese los grados celsius";
	Leer celsius;
	x <- 1.8;
	z <- 32;
	fahrenheit <- celsius * x + z;
	Escribir "su temperatura en grados fahrenheit es: "  , fahrenheit;
	
	
FinAlgoritmo
